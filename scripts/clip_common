DPKG_ERR_FILE="/var/log/dpkg.err"

work_out_confdebs () {
    [[ -n "${CLIP_CONFIGURATION}" ]] || error "Missing main configuration package name"
    
    base="$(resolve_deps ${CLIP_CONFIGURATION} ${CLIP_EXTRA_PKG})"
}


